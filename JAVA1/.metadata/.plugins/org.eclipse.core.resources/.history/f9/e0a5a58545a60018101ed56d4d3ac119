package unidade2;

public class EstruturasRepeticao {
	
	public static void main(String[] args) {
		
		int x = 10;
		
		//Verifica a condição antes do looping começar
		while(x < 10) {
			System.out.println("X = " + x);
			x++;
		}		
		
		System.out.println();
		
		//Executa o looping primeiro para só depois verificar a condição
		do {
			System.out.println("X = " + x);
			x++;
			
		}while(x < 10);
		
		for(x = 0; x < 10; x++)
			System.out.print(x + " ");
		
	}

}
