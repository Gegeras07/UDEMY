package unidade2;

public class EstruturasRepeticao {
	
	public static void main(String[] args) {
		
		int x = 10;
		
		while(x < 10) {
			System.out.println("X = " + x);
			x++;
		}
		
		x = 10;
		System.out.println();
		
		do {
			System.out.println("X = " + x);
			x++;
			
		}while(x < 10);
		
	}

}
