package unidade2;

import java.util.Scanner;

public class Harmonic {

	public static void main(String[] args) {
		
		int n = 0;
		int h = 1;
		Scanner input = new Scanner(System.in);
		
		System.out.printf("Informe um dado n: ");
		n = input.nextInt();
		
		for(int i = 2; i < n; i++)
			h += 1/i;
		
		System.out.printf("Harmonic(%d) = %d", n, h);
		
		input.close();

	}

}
